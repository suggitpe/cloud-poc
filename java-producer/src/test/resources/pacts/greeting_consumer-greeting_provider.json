{
    "provider": {
        "name": "greeting_provider"
    },
    "consumer": {
        "name": "greeting_consumer"
    },
    "interactions": [
        {
            "description": "Request for a greeting",
            "request": {
                "method": "GET",
                "path": "/greeting"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "id": 1,
                    "from": "producer",
                    "greeting": "Hello, World"
                }
            },
            "providerStates": [
                {
                    "name": "test GET"
                }
            ]
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.19"
        }
    }
}